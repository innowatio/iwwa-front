{
  "name": "iwwa-front",
  "version": "1.0.0",
  "description": "Innowatio app front-end",
  "main": "index.js",
  "scripts": {
    "build": "gulp build",
    "build-cordova": "node scripts/build-cordova.js",
    "dev": "gulp dev",
    "emulate-ios": "cd builds/cordova-app && cordova emulate ios --target=\"iPad-Air\"",
    "lint": "eslint app/**/*.js app/**/*.jsx",
    "generate-reports": "node scripts/generate-reports.js",
    "publish-reports": "node scripts/generate-reports.js && node scripts/publish-reports.js",
    "prestart": "gulp build",
    "start": "node scripts/server.js",
    "test": "NODE_PATH=app:test mocha --compilers jsx:babel/register test/unit/**/*unit*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innowatio/iwwa-front.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/innowatio/iwwa-front/issues"
  },
  "homepage": "https://github.com/innowatio/iwwa-front#readme",
  "devDependencies": {
    "babel": "^5.6.7",
    "babel-core": "^5.6.4",
    "babel-loader": "^5.1.4",
    "bluebird": "^2.9.30",
    "browser-sync": "^2.7.12",
    "chai": "^3.0.0",
    "chalk": "^1.1.1",
    "cordova": "^5.3.1",
    "dev-ip": "^1.0.1",
    "eslint": "^0.23.0",
    "eslint-html-reporter": "^0.4.11",
    "eslint-plugin-react": "^2.5.2",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.5.2",
    "gulp-eslint": "^0.14.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-if": "^1.2.5",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-minify-css": "^1.1.6",
    "gulp-preprocess": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-spawn-mocha": "^2.2.1",
    "gulp-util": "^3.0.5",
    "jsdom": "^5.4.3",
    "json-loader": "^0.5.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "mochawesome": "^1.2.0",
    "node-libs-browser": "^0.5.2",
    "pro-gulp": "^0.3.0",
    "proxyquire": "^1.5.0",
