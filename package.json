    "bootstrap": "^3.3.5",
    "color": "^0.10.1",
    "compression": "^1.5.0",
    "dygraphs": "^1.1.1",
    "express": "^4.13.0",
    "fastclick": "^1.0.6",
    "font-awesome": "^4.3.0",
    "halogen": "^0.1.10",
    "history": "^1.8.4",
    "immutable": "^3.7.4",
    "intro.js": "^1.1.1",
    "lodash.debounce": "^3.1.1",
    "lodash.throttle": "^3.0.4",
    "moment": "^2.10.3",
