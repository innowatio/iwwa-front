{
  "name": "iwwa-front",
  "version": "1.0.0",
  "description": "Innowatio app front-end",
  "main": "index.js",
  "scripts": {
    "build": "gulp build",
    "build-cordova": "node scripts/build-cordova.js",
    "dev": "gulp dev",
    "predeploy": "ENVIRONMENT=prod npm run build",
    "deploy": "asd-upload builds/prod/",
    "emulate-ios": "cd builds/cordova-app && cordova emulate ios --target=\"iPad-Air\"",
    "lint": "eslint app/**/*.js app/**/*.jsx app/**/**/*.jsx",
    "generate-reports": "node scripts/generate-reports.js",
    "publish-reports": "node scripts/generate-reports.js && node scripts/publish-reports.js",
    "prestart": "gulp build",
    "start": "node scripts/server.js",
    "test": "NODE_ENV=test NODE_PATH=app:test mocha --compilers jsx:babel-core/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innowatio/iwwa-front.git"
  },
