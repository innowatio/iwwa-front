      "cordova-plugin-device",
      "cordova-plugin-inappbrowser",
      "cordova-plugin-statusbar",
      "https://github.com/robertklein/cordova-ios-security.git"
    ]
  },
  "optionalDependencies": {
    "ios-sim": "^5.0.1"
  }
